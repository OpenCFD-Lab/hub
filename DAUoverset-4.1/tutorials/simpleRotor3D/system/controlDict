startTime       0;

stopAt          endTime;

endTime         0.1; //0.001;

deltaT          0.0025;

writeControl    timeStep;
writeInterval   40;

//writeControl    adjustable;
//writeInterval   0.01;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           1;


